{
    "version": 4,
    "configurations": [
        {
            "name": "Mac",
            "compileCommands": "${workspaceFolder}/build/Debug/compile_commands.json",
            "defines": [],
            "includePath": ["${workspaceFolder}/**"],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX15.2.sdk/System/Library/Frameworks",
                "/opt/homebrew/lib"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-arm64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-isystem", "/Library/Developer/CommandLineTools/SDKs/MacOSX15.2.sdk/usr/include/c++/v1",
                "-isystem", "/opt/homebrew/include",
                "-iframework", "/opt/homebrew/lib"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "/Library/Developer/CommandLineTools/SDKs/MacOSX15.2.sdk/usr/include",
                    "/Library/Developer/CommandLineTools/SDKs/MacOSX15.2.sdk/usr/include/c++/v1",
                    "/Library/Developer/CommandLineTools/usr/include/c++/v1",
                    "/opt/homebrew/include",
                    "/opt/homebrew/lib/QtCore.framework/Headers",
                    "/opt/homebrew/lib/QtWidgets.framework/Headers",
                    "/opt/homebrew/lib/QtGui.framework/Headers",
                    "/opt/homebrew/lib/QtCharts.framework/Headers"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}
